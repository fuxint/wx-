.bgimg{
  width: 100%;
  height:300rpx;
  border: 1px solid #ccc
}
.userinfo{
  width: 100%
}
.cart{
  padding: 10rpx 20rpx;
  position: fixed;
  top: 200rpx;
  width: 80%;
  margin: 0 auto;
  background: #fff;
  border: 1px solid #ccc
}
.name{
  font-weight: bolder;
  font-size: 44rpx;
  line-height: 80rpx
}
.tel{
  line-height: 60rpx;
  margin-bottom: 20rpx;
  font-size:30rpx;
  display: flex;
  justify-content: center;
  
}
.tel>text{
  width: 15%;
  text-align: left;
  color: #666;
}
.tel>view{
  width:50%;
  color: #666;
}
.tel>button{
  height: 60rpx;
  line-height: 60rpx;
  padding: 0rpx 30rpx;
  font-size: 30rpx;
}
.content{
  height: 200rpx;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}
.content>view{
  width: 33.33%;
}
.content>view>image{
  width: 50%;
  display: block;
  margin: 0 auto;
  margin-top: 300rpx;
  height: 80rpx;

}
.content>view>text{
  display: block;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx
}